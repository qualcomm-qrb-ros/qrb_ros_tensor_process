  <version>1.0.0</version>
  <description>Provided ROS nodes for pre-processing in CV AI models.</description>
  <maintainer email="xiaolee@qti.qualcomm.com">Xiao Li</maintainer>
  <license>BSD-3-Clause-Clear</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <!-- Build Dependencies -->
  <depend>rclcpp</depend>
  <depend>libopencv-dev</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>cv_bridge</depend>
  <depend>qrb_ros_transport_image_type</depend>
  <depend>lib_mem_dmabuf</depend>
  <depend>rclcpp_components</depend>
  <depend>qrb_ros_tensor_list_msgs</depend>

  <test_depend>ament_lint_auto</test_depend>

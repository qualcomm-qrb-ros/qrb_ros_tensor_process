<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>qrb_ros_yolo_processor</name>
  <version>1.0.0</version>
  <description>Contains ROS nodes for Yolo pre/post process</description>
  <maintainer email="quic_xiaolee@quicinc.com">Xiao Li</maintainer>
  <license>BSD-3-Clause-Clear</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <!-- Build Dependencies -->
  <depend>rclcpp</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>vision_msgs</depend>
  <depend>cv_bridge</depend>
  <depend>message_filters</depend>
  <depend>rclcpp_components</depend>

  <depend>qrb_ros_tensor_list_msgs</depend>
  <depend>qrb_yolo_processor</depend>
  <depend>qrb_ros_vision_msgs</depend>

  <!-- Runtime Dependencies -->
  <!-- <exec_depend>std_msgs</exec_depend> -->

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
